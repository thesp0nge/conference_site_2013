%ul.nav.nav-tabs
  %li=link_to tag_icon(:list, pat(:list)), url(:sponsors, :index)
  %li.active=link_to tag_icon(:plus, pat(:new)), url(:sponsors, :new)

.tabs-content
  -form_for :sponsor, url(:sponsors, :create), :class => 'form-horizontal' do |f|
    =partial 'sponsors/form', :locals => { :f => f }
